<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <managed-bean>
        <managed-bean-name>Group1Bean</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooks_amg.jsf.Group1Bean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>BookBean</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooksfinal.jsf.BookBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>UserBean</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooksfinal.jsf.UserBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>BookConverter</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooksfinal.jsf.BookBeanConverter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>BookConverter</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooksfinal.jsf.UserBeanConverter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>BookConverter</managed-bean-name>
        <managed-bean-class>com.thagoondaddy.mavenbooksfinal.jsf.Group1BeanConverter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
